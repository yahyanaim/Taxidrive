{
  "name": "taxidrive",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "concurrently -n backend,frontend -c blue,green \"pnpm --filter backend dev\" \"pnpm --filter frontend dev\"",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepare": "husky || true"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.17.0",
