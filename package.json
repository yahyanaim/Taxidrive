    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm -w eslint --fix",
      "pnpm -w prettier --write"
    ],
    "**/*.{json,md,yml,yaml,css,html}": [
      "pnpm -w prettier --write"
    ]
